<Page x:Class="DataEntryManager.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="Administrator">

    <Grid Height="170" Width="180">
        <Button Content="Login" Margin="0,140,0,0" VerticalAlignment="Top" HorizontalAlignment="Center"  Click="Button_Click"/>
        <Label Content="Username" Margin="0,5,0,0" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <TextBox Name="username" Height="23" Margin="45,32,46,0" VerticalAlignment="Top"/>
        <Label Content="Password" Margin="0,65,0,0" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <TextBox Name="password" Height="23" Margin="45,100,46,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
